<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="robot_id" default="" />
  <arg name="robot_ns" value="multirotor$(arg robot_id)" />
  <arg name="hokuyo" default="True" />
  <arg name="shift_x" default="0.0"/>
  <arg name="shift_y" default="0.0"/>
  <!-- <arg name="real_machine" default="True" /> -->
  <!-- <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" launch-prefix="python -m pdb"> -->

  <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" >
    <param name="ppo_path" value="$(find agile_multirotor)/gazebo_policy/RecurrentPPO_153" />
    <param name="hokuyo" value="$(arg hokuyo)" />
    <param name="shift_x" type="double" value="$(arg shift_x)" />
    <param name="shift_y" type="double" value="$(arg shift_y)" />
    <!-- <param name="real_transition" type="double" value="2.0" if="$(arg real_machine)"  />
    <param name="real_transition" type="double" value="0.0" unless="$(arg real_machine)"  /> -->
  </node>
</launch>