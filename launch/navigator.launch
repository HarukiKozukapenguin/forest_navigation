<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="robot_id" default="" />
  <arg name="robot_ns" default="multirotor$(arg robot_id)" />
  <arg name="hokuyo_raw_data" default="False" />
  <arg name="fixed_flight" default="False"/>
  <arg name="real_area" default="True" />
  <arg name="policy" default="RecurrentPPO_287" />
  <arg name="min_start_obstacles" default="0" />
  <arg name="body_move_area_y" default="0.10" />
  <!-- <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" launch-prefix="python -m pdb"> -->

  <group ns="$(arg robot_ns)">
    <rosparam file="$(find agile_multirotor)/config/ObstacleAvoidance.yaml" command="load" />
  </group>

  <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" >
    <param name="ppo_path" value="$(find forest_navigation)/exec_policy/$(arg policy)" />
    <param name="hokuyo" value="$(arg hokuyo_raw_data)" />
    <param name="fixed_flight" type="bool" value="$(arg fixed_flight)" />
    <param name="robot_ns" value="$(arg robot_ns)" />
    <param name="real_area" type="bool" value="$(arg real_area)" />
    <param name="min_start_obstacles" type="int" value="$(arg min_start_obstacles)" />
    <param name="body_move_area_y" type="double" value="$(arg body_move_area_y)" />
  </node>
</launch>
