<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="robot_id" default="" />
  <arg name="robot_ns" default="multirotor$(arg robot_id)" />
  <arg name="hokuyo" default="True" />
  <arg name="shift_x" default="0.0"/>
  <arg name="shift_y" default="0.0"/>
  <arg name="body_r" default="0.25"/>
  <arg name="fixed_flight" default="False"/>
  <arg name="time_constant" default="0.510"/>
  <arg name="delay" default="False" />
  <arg name="real_area" default="True" />
  <arg name="same_gain_in_exec_policy" default="True" />
  <!-- <arg name="real_machine" default="True" /> -->
  <!-- <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" launch-prefix="python -m pdb"> -->

  <node pkg="forest_navigation" type="policy_navigator.py" name="policy_navigator" output="screen" ns="$(arg robot_ns)" >
    <param name="ppo_path" value="$(find agile_multirotor)/gazebo_policy/RecurrentPPO_258" />
    <param name="hokuyo" value="$(arg hokuyo)" />
    <param name="shift_x" type="double" value="$(arg shift_x)" />
    <param name="shift_y" type="double" value="$(arg shift_y)" />
    <param name="body_r" type="double" value="$(arg body_r)" />
    <param name="fixed_flight" type="bool" value="$(arg fixed_flight)" />
    <param name="robot_ns" value="$(arg robot_ns)" />
    <param name="time_constant" value="$(arg time_constant)" />
    <param name="delay" type="bool" value="$(arg delay)" />
    <param name="real_area" type="bool" value="$(arg real_area)" />
    <param name="same_gain_in_exec_policy" type="bool" value="$(arg same_gain_in_exec_policy)" />
    <!-- <param name="real_transition" type="double" value="2.0" if="$(arg real_machine)"  />
    <param name="real_transition" type="double" value="0.0" unless="$(arg real_machine)"  /> -->
  </node>
</launch>